<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/grid2">

    <ImageView
        android:id="@+id/Student"
        android:layout_width="75px"
        android:layout_height="75px"
        android:layout_marginStart="605px"
        android:layout_marginTop="400px"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0"
        app:srcCompat="@drawable/minerhat" />
    //student

    <ImageButton
        android:id="@+id/AUp"
        android:layout_width="532dp"
        android:layout_height="537dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="600dp"
        android:onClick="ArrowUp"
        android:scaleX=".1"
        android:scaleY=".1"
        android:src="@drawable/arrowu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.54"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.53"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/ALeft"
        android:layout_width="532dp"
        android:layout_height="537dp"
        android:layout_marginTop="102dp"
        android:layout_marginEnd="609dp"
        android:onClick="ArrowLeft"
        android:rotation="-90"
        android:scaleX=".1"
        android:scaleY=".1"
        android:src="@drawable/arrowu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.71"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.29000002"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/ARight"
        android:layout_width="532dp"
        android:layout_height="537dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="600dp"
        android:onClick="ArrowRight"
        android:rotation="+90"
        android:scaleX=".1"
        android:scaleY=".1"
        android:src="@drawable/arrowu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.34"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.29000002"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/ADown"
        android:layout_width="532dp"
        android:layout_height="537dp"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="604dp"
        android:onClick="ArrowDown"
        android:rotation="180"
        android:scaleX=".1"
        android:scaleY=".1"
        android:src="@drawable/arrowu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.31"
        tools:ignore="SpeakableTextPresentCheck" />


    <ImageView
        android:id="@+id/Boulder1"
        android:layout_width="75px"
        android:layout_height="75px"
        android:layout_marginStart="10px"
        android:layout_marginTop="60px"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0"
        app:srcCompat="@drawable/boulder" />//boulders

    <ImageView
        android:id="@+id/Boulder2"
        android:layout_width="75px"
        android:layout_height="75px"
